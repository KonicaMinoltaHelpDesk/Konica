        / = simple_form_for @consommable, url: search_consommables_path(@consommable), method: :get do |f| 
        /   / = f.autocomplete_field :code_incidents, autocomplete_code_incidents_incidents_path,:size => 75, :"data-autocomplete-label" => false,placeholder: "Entrez le code incident "
