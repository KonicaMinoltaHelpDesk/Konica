        %p Entrez le code incident a rechercher :
        = simple_form_for @incident, url: search_incidents_path(@incident), method: :get do |f| 
          / = f.autocomplete_field :code_incidents, autocomplete_code_incidents_incidents_path,:size => 75, :"data-autocomplete-label" => false,placeholder: "Entrez le code incident "
          = f.autocomplete_field :code_incidents, autocomplete_incident_code_incidents_pages_path,:size => 75, :"data-autocomplete-label" => false,placeholder: "Entrez le code incident "


        / = simple_form_for @incident, url: ajax_incident_incidents_path(@incident) do |f|

        /   = f.input :code_incidents, as: :string, :input_html => { :style => "width:500px" }, style: "display:none;", label: false, placeholder: "code incidents", autocomplete: "off"


