%table{:align => "center", :style => "width: 500px;"}
  %thead
    %tr
      %th{:style => "vertical-align: center;"}
        CodeIncident
      %th{:align => "center"}
        Intitule
  %tbody
    - @printers.each do |printer|
      %tr
        %td{:align => "center"}
          = link_to printer.code_printers
        %td{:align => "center"}
          = link_to  printer.intitulePrinters
